{
  "name": "Moti",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "create-react-app for extensions",
  "icons": {
    "128": "logo192.png"
  },
  "permissions": [
    "webRequestBlocking",
    "webRequest",
    "activeTab",
    "tabs",
    "http://*/*",
    "https://*/*",
    "*://*.google.com/",
    "storage",
    "background"
 ],
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  
  "browser_action": {
    "default_icon": "logo512.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  }
}
